<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.login.LoginFormController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1000.0" layoutX="-1.0" layoutY="-1.0">
         <image>
            <Image url="@../images/Blue%20Modern%20Login%20Page%20Desktop%20Prototype%20(1).png" />
