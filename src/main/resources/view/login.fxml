         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Text>
      <Text layoutX="638.0" layoutY="414.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password*">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Text>
      <PasswordField fx:id="txtPasswordId" layoutX="638.0" layoutY="433.0" prefHeight="30.0" prefWidth="305.0" />
      <Button layoutX="638.0" layoutY="495.0" mnemonicParsing="false" onAction="#btnLoginAction" prefHeight="30.0" prefWidth="305.0" style="-fx-background-color: #d9d19e;" text="LOGIN">
